{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "devops-files-app",
        "image": "chaqattack/devops-files-app",
        "environment": [
          {
            "DB_ENV": "$production"
          }
        ],
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 3081,
            "containerPort": 3081
          }
        ]
      },
      {
        "name": "devops-users-app",
        "image": "chaqattack/devops-users-app",
        "environment": [
          {
            "ENV" : "production"
      
          }
        ],
        "essential": true,
        "memory": 128, 
        "portMappings": [
            {
              "hostPort": 3082,
              "containerPort": 3082
            }
        ],
        "links": [
          "devops-files-app"
        ]
      }
    ]
  }